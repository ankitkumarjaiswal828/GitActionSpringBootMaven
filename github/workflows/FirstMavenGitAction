name: My First Workflow




on: 
 push:
 workflow_dispatch:

jobs:
  echo-print:
   runs-on: ubuntu-latest
   

        

   steps:
    - uses: actions/checkout@v4
    - uses: actions/setup-java@v4.5.0
      with:
       java-version: '21'
       distribution: 'temurin'
    - run: mvn --batch-mode --update-snapshots verify
    - run: mkdir staging && cp target/*.jar staging
